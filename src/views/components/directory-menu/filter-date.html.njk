<li class="subcategory-item filter">
  <div class="name-wrapper {{ 'expanded' if filter.expanded_by_default else ''}}">
    <span class="name">{{ filter.label }}</span>
    <div class="arrow-after"></div>
  </div>
  <div class="btn-clear-filter" data-id={{ filter.id }}>
    <span class="label">{{ 'clear'|i18n }}</span>
    <span class="gogo-icon-close"></span>
  </div>
</li>

<div class="filter-wrapper gogo-date-filter "
     data-id={{ filter.id }} style="{{ '' if filter.expanded_by_default else 'display: none' }}">
  {% if filter.subtype == "range" %}

  {% endif %}
</div>

<div class="filter-wrapper gogo-date-filter" data-id={{ filter.id }} style="{{ '' if filter.expanded_by_default else 'display: none' }}">

  <div class="view-selection" {% if filter.views|length <= 1 %}style="display: none"{% endif %}>
    {% for view in filter.views %}
      <button class="view-type {{ 'active' if filter.defaultView == view else ''}}" data-name="{{ view }}">{{ ('filter.date.view.' ~ view)|i18n}}</button>
    {% endfor %}
  </div>

  <div class="view-filters">
    {% for view in filter.views %}
      <div class="view-filter {{ 'input-daterange input-group' if view == "range" else ''}}" data-name="{{ view }}">
        {% if view == "range" %}
          <div class="range-container">
            <input type="text" class="input-sm form-control" name="start" />
            <span class="input-group-addon col s2">to</span>
            <input type="text" class="input-sm form-control" name="end" />
          </div>
        {% endif %}
      </div>
    {% endfor %}
  </div>
</div>